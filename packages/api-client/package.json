{
  "name": "@cher1shrxd/api-client",
  "version": "0.0.1",
  "description": "A flexible API client for Next.js with server-side cookie support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts"
  },
  "peerDependencies": {
    "axios": ">=1.0.0",
    "next": ">=13.0.0"
  },
  "devDependencies": {
    "axios": "^1.7.0",
    "next": "^15.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cher1shrxd/cher1shrxd-utils"
  },
  "keywords": [
    "api-client",
    "axios",
    "nextjs",
    "ssr",
    "cookies"
  ],
  "author": "cher1shrxd",
  "license": "MIT"
}
